{
  "name": "EVO",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "http-server dist/",
    "watch": "webpack-dev-server --inline --progress --port 3000",
    "preinstall": "npm install -g http-server",
    "postinstall": "npm run build-dev",
    "dev-with-prod-proxy": "webpack-dev-server --inline --progress --port 3000 --env.prodProxy --env.mode=dev",
    "test": "karma start karma.conf.js",
    "test:dev": "karma start karma.conf.js --auto-watch --no-single-run",
    "test:coverage": "karma start karma.conf.js --env.coverage",
    "test:ci": "karma start ./config/ci.karma.conf.js",
    "lint": "./node_modules/.bin/tslint -c tslint.json 'src/**/*.ts'",
    "build": "rm -rf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "build-dev": "rm -rf dist && webpack --config config/webpack.prod.js --progress --profile --bail --env.mode=dev",
    "build-prod": "rm -rf dist && webpack --config config/webpack.prod.js --progress --profile --bail --env.mode=prod"
  },
  "license": "ISC",
