    "lint": "./node_modules/.bin/tslint -c tslint.json 'src/**/*.ts'",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "build-dev": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail --env.mode=dev",
    "build-prod": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail --env.mode=prod"
